[32m+[m[32m        miseAJourSanteEfficacite(&(ruche->ouvrieres[i]));[m
[32m+[m[32m        mettreAJourRole(ouvriere);[m
[32m+[m[32m        effectuerTache(ouvriere);[m
[32m+[m[32m    }[m
